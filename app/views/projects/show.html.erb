<div id="post_show_content" class="skinny_wrapper wrapper_padding">
	<header>
			<p class="date"><%= @project.created_at.strftime('%A, %B %d') %></p>
			<h1><%= @project.title %></h1>
			<hr>
	</header>


<div id="content">
		<%=  @project.description %>

		<br>
		<div class="button_wrapper">
		<a href="<%= @project.link %>" class="project_button" target="blank">Visit <%= @project.title %> </a>
		</div>
	</div>
<hr>
</div>